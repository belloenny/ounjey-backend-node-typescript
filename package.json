{
  "name": "ounjey",
  "version": "1.0.0",
  "description": "backend service for ounjey.com",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t ounjey .",
    "docker:run": "docker run -p 3000:3000 -d ounjey",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "NODE_ENV=development tsc-watch --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^4.2.9",
    "@loopback/authentication-passport": "^2.1.9",
    "@loopback/authorization": "0.6.2",
    "@loopback/boot": "^2.3.5",
    "@loopback/core": "^2.9.1",
    "@loopback/openapi-v3": "^3.4.5",
    "@loopback/repository": "^2.9.0",
    "@loopback/rest": "^5.2.0",
    "@loopback/rest-crud": "^0.8.9",
    "@loopback/rest-explorer": "^2.2.6",
    "@loopback/security": "^0.2.14",
    "@loopback/service-proxy": "^2.3.4",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/client-sessions": "^0.8.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/helmet": "^0.0.47",
    "@types/passport": "^1.0.4",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-google-oauth2": "^0.1.3",
    "@types/passport-http": "^0.3.8",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2": "^1.4.9",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "client-sessions": "^0.8.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.19",
    "loopback-connector-mongodb": "^5.2.3",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "path": "^0.12.7",
    "ts-node-dev": "^1.0.0-pre.52",
    "tsc-watch": "^4.2.9",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.1.0",
    "@loopback/eslint-config": "^8.0.3",
    "@loopback/testlab": "^3.2.0",
    "@types/node": "^10.17.26",
    "eslint": "^7.3.1",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.5"
  }
}
